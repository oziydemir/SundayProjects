public class MathPrice {
    public decimal practiceOne(decimal a,decimal b) {
        decimal cevre=2*a+2*b;
        return cevre;
    }

    public decimal practiceTwo(decimal height, decimal weight){
        decimal bci=weight/(height*height);
        return bci;

    }

    public Decimal practiceThree(Decimal a, Decimal b){
        Decimal average=(a+b)/2;
        return average;
    }
}
